<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/gray_168">
    <item>
        <selector>
            <item android:state_pressed="true">
                <shape android:shape="rectangle">
                    <corners
                        android:bottomLeftRadius="@dimen/d0"
                        android:bottomRightRadius="@dimen/d0"
                        android:topLeftRadius="@dimen/d4"
                        android:topRightRadius="@dimen/d4" />
                    <solid android:color="@color/gray_027" />
                </shape>
            </item>

            <item>
                <shape android:shape="rectangle">
                    <corners
                        android:bottomLeftRadius="@dimen/d0"
                        android:bottomRightRadius="@dimen/d0"
                        android:topLeftRadius="@dimen/d4"
                        android:topRightRadius="@dimen/d4" />
                    <solid android:color="@color/white" />
                </shape>
            </item>

        </selector>
    </item>
</ripple>
